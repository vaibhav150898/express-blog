<%- include('header.ejs')%>

<section class="featured-blog mtb-40">
  <div class="container">
    <div class="row">
      <% n.forEach((b)=>{ %>
      <div class="col-md-4">
        <div class="blog-box">
          <img
            src="<%= b.image.url %>"
            alt="blog1"
            width="250px"
            height="250px"
          />

          <h3><%= b.title %></h3>

          <p><%= b.description.slice(0,90) %></p>
          <a href="" class="btn btn-danger">Read more...</a>
        </div>
      </div>
      <% }) %>
    </div>
  </div>
</section>

<%- include('footer.ejs')%>
